<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="hr_interinale_contatti" model="ir.actions.act_window">
        <field name="name">Contatti aziende interinale</field>
        <field name="res_model">hr.interinale.contatti</field>
        <field name="view_mode">tree</field>
    </record>

    <!-- Relativo alla visualizzazione della tabella ad alberlo -->
    <record id="hr_interinale_contatti_views_tree" model="ir.ui.view">
        <field name="name">Contatti aziende interinale</field>
        <field name="model">hr.interinale.contatti</field>
        <field name="arch" type="xml">
            <tree string="Contatti aziende interinale" editable="bottom">
                <field name="res_partner_id"/>
                <field name="res_cdc_id"/>
                <!-- <field name="email"/> -->
            </tree>
        </field>
    </record>


</odoo>